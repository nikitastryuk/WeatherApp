{
  "name": "weather-forecast-app",
  "version": "1.0.0",
  "description": "AngularJS TS & WebPack4 Weather Forecast App",
  "scripts": {
    "start:dev": "webpack-dev-server --config webpack-dev.config.js",
    "start:prod": "npm run build:prod && http-server",
    "build:dev": "rimraf .tmp && webpack --config webpack-dev.config.js",
    "build:prod": "rimraf public && webpack --config webpack-prod.config.js"
  },
