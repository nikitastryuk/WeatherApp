  "license": "MIT",
  "devDependencies": {
    "@types/angular": "1.6.54",
    "@types/angular-ui-router": "1.1.40",
    "@types/core-js": "2.5.0",
    "@types/node": "12.0.2",
    "@types/webpack-env": "1.13.9",
    "awesome-typescript-loader": "5.2.1",
    "core-js": "3.0.1",
    "css-loader": "2.1.1",
    "extract-text-webpack-plugin": "v4.0.0-beta.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "ng-annotate-loader": "0.6.1",
    "node-sass": "4.12.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "tslint": "5.16.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.4.5",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.22",
    "angular": "1.7.8",
    "bootstrap": "4.3.1"
  }
}
